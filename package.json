{
  "name": "alice-cloud-proxy",
  "version": "0.1.0",
  "description": "Universal cloud function to proxy Alice skill requests",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/alice-cloud-proxy.git"
  },
  "scripts": {
    "code": "eslint src test",
    "test": "mocha test/setup.js test/*.spec.js",
    "test:cov": "nyc --reporter=html --check-coverage --lines 100 npm t && nyc report"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:cov",
      "pre-push": "npm run code && npm run test:cov"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.1",
    "eslint-plugin-mocha": "^6.0.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.2.3",
    "mocha": "^6.2.0",
    "nock": "^11.3.6",
    "nyc": "^14.1.1"
  },
  "license": "MIT"
}
